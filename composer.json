{
    "name": "okaufmann/laravel-git-version",
    "description": "A helper to get the current git version of the application",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel",
        "git",
        "version"
    ],
    "require": {
        "laravel/framework": ">=4"
    },
    "authors": [
        {
            "name": "Bart Nagel",
            "email": "bart@tremby.net"
        },
        {
            "name": "Oliver Kaufmann",
            "email": "okaufmann91@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tremby\\LaravelGitVersion\\": "src/"
        }
    },
    "extra": {
      "laravel": {
          "providers": [
              "Tremby\\LaravelGitVersion\\GitVersionServiceProvider"
          ]
      }
   }
}
